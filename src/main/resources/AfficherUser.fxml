            <TableColumn fx:id="email" prefWidth="62.0" text="email" />
            <TableColumn fx:id="numTel" prefWidth="65.0" text="numTel" />
            <TableColumn fx:id="preference" prefWidth="96.0" text="preference" />
            <TableColumn fx:id="localisation" prefWidth="95.0" text="localisation" />
        </columns>
      </TableView>
      <Button fx:id="ajoutU" layoutX="36.0" layoutY="301.0" mnemonicParsing="false" onAction="#ajouterUser" style="-fx-background-color: #F99D26;" text="Ajouter utilisateur" textFill="WHITE" />
